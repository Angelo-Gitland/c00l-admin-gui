local Players = game:GetService("Players")
local player = Players.LocalPlayer

local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "c00lkiddCmds"
gui.ResetOnSpawn = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 400, 0, 360)
frame.Position = UDim2.new(0.5, -200, 0.5, -180)
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.BorderColor3 = Color3.new(1, 0, 0)
frame.Active = true
frame.Draggable = true

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0, 40)
title.BackgroundTransparency = 1
title.Text = "c00lkidd Cmds By Lua Land"
title.Font = Enum.Font.SourceSansBold
title.TextSize = 24
title.TextColor3 = Color3.new(1, 0, 0)

spawn(function()
	while true do
		for i = 0, 1, 0.01 do
			title.TextColor3 = Color3.fromHSV(i, 1, 1)
			wait(0.05)
		end
	end
end)

local scroll = Instance.new("ScrollingFrame", frame)
scroll.Size = UDim2.new(1, -20, 0, 120)
scroll.Position = UDim2.new(0, 10, 0, 50)
scroll.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
scroll.BorderColor3 = Color3.new(1, 0, 0)
scroll.CanvasSize = UDim2.new(0, 0, 0, 200)
scroll.ScrollBarThickness = 6

local list = Instance.new("TextLabel", scroll)
list.Size = UDim2.new(1, -10, 0, 200)
list.Position = UDim2.new(0, 5, 0, 0)
list.BackgroundTransparency = 1
list.Font = Enum.Font.SourceSans
list.TextSize = 18
list.TextColor3 = Color3.new(1, 1, 1)
list.TextWrapped = true
list.TextYAlignment = Enum.TextYAlignment.Top
list.TextXAlignment = Enum.TextXAlignment.Left
list.Text = [[
Available Commands:
Fly
Dex
]]

local inputFrame = Instance.new("Frame", frame)
inputFrame.Size = UDim2.new(1, -20, 0, 40)
inputFrame.Position = UDim2.new(0, 10, 0, 180)
inputFrame.BackgroundColor3 = Color3.new(0, 0, 0)
inputFrame.BorderColor3 = Color3.new(1, 0, 0)
inputFrame.ClipsDescendants = true

local prefixLabel = Instance.new("TextLabel", inputFrame)
prefixLabel.Size = UDim2.new(0, 20, 1, 0)
prefixLabel.Position = UDim2.new(0, 5, 0, 0)
prefixLabel.BackgroundTransparency = 1
prefixLabel.Font = Enum.Font.SourceSansBold
prefixLabel.TextSize = 20
prefixLabel.TextColor3 = Color3.new(1, 0, 0)
prefixLabel.Text = "/"

local commandBox = Instance.new("TextBox", inputFrame)
commandBox.Size = UDim2.new(1, -30, 1, 0)
commandBox.Position = UDim2.new(0, 25, 0, 0)
commandBox.BackgroundTransparency = 1
commandBox.Font = Enum.Font.SourceSans
commandBox.TextSize = 20
commandBox.TextColor3 = Color3.new(1, 1, 1)
commandBox.Text = ""
commandBox.ClearTextOnFocus = false

commandBox:GetPropertyChangedSignal("Text"):Connect(function()
	if #commandBox.Text > 40 then
		commandBox.Text = commandBox.Text:sub(1, 40)
	end
end)

local run = Instance.new("TextButton", frame)
run.Size = UDim2.new(1, -20, 0, 40)
run.Position = UDim2.new(0, 10, 0, 230)
run.BackgroundColor3 = Color3.new(1, 0, 0)
run.Font = Enum.Font.SourceSansBold
run.TextSize = 20
run.TextColor3 = Color3.new(1, 1, 1)
run.Text = "RUN"

local commandLinks = {
	["fly"] = "https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt",
	["dex"] = "https://raw.githubusercontent.com/infyiff/backup/main/dex.lua",
}

run.MouseButton1Click:Connect(function()
	local input = commandBox.Text:lower()
	local link = commandLinks[input]
	if link then
		loadstring(game:HttpGet(link))()
	else
		warn("Unknown command: " .. input)
	end
end)
